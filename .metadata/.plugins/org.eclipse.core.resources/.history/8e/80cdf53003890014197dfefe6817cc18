
public class LinkedListTest {

	int size;
	ListNode head;
	
	LinkedListTest(){
		size = 0;
		head = new ListNode(null);
	}
	
	int size(){
		return this.size;
	}
	
	void add(ListNode head, Object element) { //добавить элемент s к списку head
		ListNode e=new ListNode (element); //создать новый элемент списка и поместить в него s
		Node old=head.next; 
		head.next=e; 
		e.next=old; // добавить элемент
		}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

class ListNode{
	
	Object element;
	ListNode next;
	
	ListNode(Object theElement){
		element = theElement;
		next = null;
	}
	
	ListNode( Object theElement, ListNode n ) {

        element = theElement;

        next    = n;

    }
}
