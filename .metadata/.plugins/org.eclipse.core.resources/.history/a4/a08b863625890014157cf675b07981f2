import java.util.LinkedList;
import java.util.ListIterator;


public class HashMapTest<K,V>{

	LinkedList<Entry>[] list ;
	int size;
	
	HashMapTest(){
		list = new LinkedList[10];
	}
	
	public static void main(String[] args) {
		HashMapTest t = new HashMapTest();
		t.put(1, 34);
	}

	public void put(K key, V value) {
		
		Entry element = new Entry(key,value);
		int index = indexOf(key);
		LinkedList<Entry> thisList =  list[index];
		if(thisList.isEmpty()){
			thisList.add(element);
		}else{
			for(Entry e:thisList){
				if(e.key == key){
					e.value = value;
				}else{
					thisList.add(element);
				}
				
			}
		}
		
		list[index] = thisList;
		size++;	
		
		
	}
	
	int indexOf(K key){
		int a  = Math.abs(key.hashCode())%10;
		return a;
	}

	public V get(K key) {
		int index = indexOf(key);
		LinkedList<Entry> thisList =  list[index];
		V value = null ;
		for(Entry e:thisList){
			if(e.key == key){
				value =  e.value;
			}
		}
		return value;
		
	}

	public int size() {
		// TODO Auto-generated method stub
		return size;
	}

		class Entry{
			K key;
			V value;
			
			Entry(K theKey,V theValue){
				key = theKey;
				value = theValue;
			}
		}
}
